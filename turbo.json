{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": [ "^build" ],
      "outputs": [ "dist/**" ]
    },
    "web#build": {
      "dependsOn": [
        "^build"
      ],
      "env": [
        "HELIUS_KEY"
      ]
    },
    "lint": {
      "outputs": []
    },
    "dev": {
      "dependsOn": ["^db:generate"],
      "cache": false
    },
    "db:generate": {
      "cache": false
    },
    "db:push": {
      "cache": false
    },
    "db:pull": {
      "cache": false
    }
  },
  "globalDependencies": [
    ".env"
  ],
  "globalEnv": [
    "HELIUS_KEY"
  ]
}
